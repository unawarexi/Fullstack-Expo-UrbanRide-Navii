{
  "expo": {
    "name": "Navii",
    "slug": "Navii",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand/logo-Photoroom.png",
    "scheme": "fullstackexpourbanridenavii",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.FullstackExpoUrbanRideNavii"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.FullstackExpoUrbanRideNavii"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
